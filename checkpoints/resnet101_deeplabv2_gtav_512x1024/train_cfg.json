    "test_dataset": {
        "name": "CityscapesSegmentation",
        "image_set": "train",
        "root": "..\/..\/dataset\/cityscapes"
    },
    "loss": {
        "name": "torch_loss",
        "torch_loss_class": "CrossEntropyLoss",
        "ignore_index": 255
    },
    "optimizer": {
        "name": "torch_optimizer",
        "torch_optim_class": "SGD",
        "lr": 0.002,
        "momentum": 0.9,
        "weight_decay": 0.0005
    },
    "lr_scheduler": {
        "name": "poly_scheduler",
        "epochs": 10,
        "power": 0.9
    },
    "train": {
        "exp_name": "resnet101_deeplabv2_gtav_512x1024",
