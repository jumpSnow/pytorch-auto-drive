        "ppl": 18,
        "thresh": 0.3,
        "collate_fn": null,
        "input_size": [
            288,
            800
        ],
        "original_size": [
            590,
            1640
        ],
        "max_lane": 4,
        "dataset_name": "culane"
    },
    "model": {
        "name": "ENet",
        "num_classes": 5,
        "encoder_relu": false,
        "decoder_relu": true,
        "dropout_1": 0.01,
        "dropout_2": 0.1,
        "encoder_only": false,
